{
  "type": "module",
  "name": "@comet-utils/openapi-generator",
  "version": "0.2.0",
  "description": "Openapi generator for comet",
  "scripts": {
    "build": "esbuild --banner:js=\"#! /usr/bin/env node\" --bundle --outfile=dist/bin.mjs --sourcemap=linked --metafile=metafile.json --platform=node --packages=external --format=esm --target=es2022 scripts/index.ts",
    "lint": "eslint --cache .",
    "typecheck": "tsc"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/bin.mjs",
  "main": "./dist/bin.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniFoldi/comet-utils.git"
  },
  "keywords": [
    "cloudflare-workers",
    "cloudflare",
    "workers",
    "routing",
    "comet",
    "router",
    "openapi",
    "openapi generator",
    "comet utils"
  ],
  "author": "DaniFoldi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaniFoldi/comet-utils/issues"
  },
  "homepage": "https://github.com/DaniFoldi/comet-utils#readme",
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.2",
    "@neoaren/comet": "^3.2.0",
    "@readme/openapi-parser": "^2.5.0",
    "citty": "^0.1.4",
    "defu": "^6.1.2",
    "esbuild": "^0.19.5",
    "urlpattern-polyfill": "^9.0.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.21.4"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/babel__traverse": "^7.20.3",
    "@types/node": "^20.8.7",
    "bundled-eslint-config": "^0.1.6",
    "eslint": "^8.52.0",
    "knip": "^2.36.0",
    "simple-knip-config": "^0.1.0",
    "some-tsconfig-preset": "^1.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
